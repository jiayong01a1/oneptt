                    />
            </TableRow>
        </TableLayout>

        <LinearLayout
            android:layout_alignParentBottom="true"
            android:layout_margin="@dimen/public_five"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <Button
                android:id="@+id/btn_one_key_child_cancel"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="取消"
                android:textColor="@color/publicWhite"
                android:gravity="center"
                android:textSize="16dip"
                android:layout_margin="5dip"
                android:padding="3dip"
                android:background="@drawable/caiji_bg_disable"
                />
            <Button
                android:id="@+id/btn_one_key_child_check"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="确认"
                android:textColor="@color/common_white"
                android:textSize="16dip"
                android:layout_margin="5dip"
                android:gravity="center"
                android:padding="3dip"
                android:background="@drawable/caiji_bg"
                />
        </LinearLayout>

