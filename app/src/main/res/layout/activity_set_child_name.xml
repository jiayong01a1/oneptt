<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/public_background"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <android.support.v7.widget.Toolbar
        android:id="@+id/key_set_child_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/light_blue"
        app:navigationIcon="@mipmap/fanhui"
        app:title="@string/one_key_set_child"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        app:titleTextAppearance="@style/toolbar_title_style"
        app:subtitleTextAppearance="@style/toolbar_subtitle_style"
        app:popupTheme="@style/toolbar_pop_theme"
        />

    <LinearLayout
        android:id="@+id/ll_one_key_set_child_a"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="@dimen/public_five"
        android:padding="5dip"
        android:background="@drawable/login_button_bg"
        >
        <TextView
            android:id="@+id/tv_one_key_set_child"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:text="@string/one_key_set_name"
            android:textColor="@android:color/black"
            android:padding="3dip"
            android:textSize="16sp"
            />
        <TextView
            android:id="@+id/tv_one_key_set_select_child"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:hint="请选择需要设置的一键通"
            android:textColorHint="@color/gray"
            android:padding="3dip"
            android:gravity="center"
            android:background="@drawable/editview_bg"
            android:drawableRight="@mipmap/sanjiaoxing"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rl_one_key_child"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow
                android:layout_margin="@dimen/public_five"
                android:padding="5dip"
                android:background="@drawable/login_button_bg"
                >
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="@dimen/public_text_size"
                    android:gravity="center"
                    android:text="@string/one_key_child_name1"
                    android:textColor="@android:color/black"
                    android:padding="3dip"
                    />
                <EditText
                    android:id="@+id/et_one_key_child_name1"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:textColorHint="@color/gray"
                    android:padding="3dip"
                    android:gravity="center"
                    android:background="@drawable/editview_bg"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/public_text_size"
                    />
            </TableRow>
            <TableRow
                android:layout_margin="@dimen/public_five"
                android:padding="5dip"
                android:background="@drawable/login_button_bg"
                >
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="@dimen/public_text_size"
                    android:gravity="center"
                    android:text="@string/one_key_child_name2"
                    android:textColor="@android:color/black"
                    android:padding="3dip"
                    />
                <EditText
                    android:id="@+id/et_one_key_child_name2"
