<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="onerfid.com.oneptt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:name=".common.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/onerfid_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity"></activity>
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.pusktotalk.OneHandleCollectActivity" />
        <activity android:name=".ui.pusktotalk.OneKeyCollectActivity" />
        <activity android:name=".ui.pusktotalk.OneKeySetActivity" />
        <activity android:name=".ui.pusktotalk.OneHandleChangeActivity" />
        <activity android:name=".ui.pusktotalk.SetChildNameActivity"></activity>
    </application>

</manifest>